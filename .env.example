# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=eu-north-1
AWS_ACCOUNT_ID=your_account_id_here

# S3 Buckets
DATA_STORAGE_BUCKET=mlops-taxi-prediction-data-storage-dev
MLFLOW_ARTIFACTS_BUCKET=mlops-taxi-prediction-mlflow-artifacts-dev
MONITORING_REPORTS_BUCKET=mlops-taxi-prediction-monitoring-reports-dev

# Kinesis Configuration
KINESIS_STREAM_NAME=taxi-ride-predictions-stream

# Lambda Configuration
LAMBDA_FUNCTION_NAME=taxi-trip-duration-predictor

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=s3://mlops-taxi-prediction-mlflow-artifacts-dev
MLFLOW_BACKEND_STORE_URI=postgresql://airflow:changeme123!@localhost:5432/mlflow

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=./airflow
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_EMAIL=admin@example.com

# ECR Configuration
ECR_REGISTRY_URL=123456789012.dkr.ecr.eu-north-1.amazonaws.com
PROJECT_NAME=mlops-taxi-prediction
IMAGE_TAG=latest

# Docker Build Configuration
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1

# Environment Settings
ENVIRONMENT=dev
LOG_LEVEL=INFO
DEBUG=false

# Terraform Variables (for CI/CD)
TF_VAR_airflow_db_password=changeme123!
TF_VAR_environment=dev
TF_VAR_project_name=mlops-taxi-prediction
